    "modulePaths": [
      "/home/user/Документы/Projects/pss-frontend/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
